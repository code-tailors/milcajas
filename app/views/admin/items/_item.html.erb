<% return unless item.denouncer_ids.empty? %>

  <tr id="<%= item.id %>" >
  <td><%= item.name %> </td>

  <td><%= item.size %></td>
  <td><%= item.mime_type %></td>

  <td>
      <span class="label label-warning"><%= item.category.try(:name) %></span>
  </td>
  <td><%= item.denounces.size %></td>
  <td><%= link_to t('models.user'), admin_user_path(item.user_id) %></td>
</tr>

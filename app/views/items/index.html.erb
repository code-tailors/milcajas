<%= form_tag '#', class: 'form-inline', remote: true do %>
   <div class="btn-group pull-right">
      <button class="btn btn-primary ">Refrescar</button>
      <button class="btn dropdown-toggle btn-primary" data-toggle="dropdown">
        <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
        <%= link_to "Refrescar", refresh_items_path,  class: "btn btn-primary" %>
        <%= link_to "Resetear", reset_items_path, class: "btn btn-danger", method: :delete %>
      </ul>
    </div>


  <div class="input-append">
    <input class="span3" id="appendedInputButtons" size="30" type="text">
    <button class="btn btn-success" type="button">Buscar</button>
  </div>

<% end %>

